pipeline {
    agent (label 'suresh')
    stages {
        stage('git-clone') {
            steps {
               git changelog: false, credentialsId: 'git', poll: false, url: 'https://github.com/glk02/Studentapp.git'
            }	
		}	
        stage('mvn-clean') {
            steps {
                sh 'mvn clean'
            }	
		}	
	    stage('mvn-compile') {
            steps {
			    sh 'mvn compile'
			}
		}	
		stage('mvn-package') {
            steps {	
			     sh 'package'
			}
			

        }
    }
}
